{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [ "." ],
        "alias": {
          "~": "./src",
          "~components": "./src/components",
          "~screens": "./src/screens"
        }
      }
    ]
  ]
}
